<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE ct_process PUBLIC "-//CISCO//DTD DITA 1.3 ct_process v1.0//EN" "cisco-ct-process.dtd">
<ct_process id="CN_Concept.dita_50c30138-041a-44e6-b6aa-8a7284367059" xml:lang="en_US">
    <title>Process Template</title>
    <!--Title: For human processes, use: ‘Gerund + [plural noun]’. For system processes, use: ‘How [items] work."-->
    <shortdesc/>
    <ct_process_body>
        <process_context>
            <p></p>
            <ul>
                <li>
                    <p/>
                </li>
            </ul>
        </process_context>
        <process_summary>
            <p><?oxy-placeholder content=" Use the list to introduce the actors or key components of this process."?></p>
            <ul>
                <li>
                    <p><?oxy-placeholder content=" Actor or component: Specify the role the actor performs, or the actions the component takes."?></p>
                </li>
            </ul>
            <p><?oxy-placeholder content=" Summary of the process."?></p>
        </process_summary>
        <process_workflow>
            <fig>
                <!--Visual representation of the process-->
                <title/>
                <image/>
            </fig>
            <p><?oxy-placeholder content=" Stem sentence for stages. For human processes, use: 'These are the stages of...'. For system processes, use: 'These stages describe...'."?></p>
            <process_stages>
                <process_stage>
                    <p><?oxy-placeholder content=" Process stage that identifies the time frame (optional), the actor, and the actions taken."?></p>
                    <ul>
                        <li>
                            <p/>
                        </li>
                    </ul>
                    <table frame="all" rowsep="1" colsep="1" outputclass="when_then">
                        <tgroup cols="4">
                            <colspec colname="c1" colnum="1" colwidth="1*"/>
                            <colspec colname="c2" colnum="2" colwidth="1*"/>
                            <colspec colname="c3" colnum="3" colwidth="1*"/>
                            <colspec colname="c4" colnum="4" colwidth="1*"/>
                            <thead>
                                <row>
                                    <entry>When...</entry>
                                    <entry>And...</entry>
                                    <entry>Then...</entry>
                                    <entry>And...</entry>
                                </row>
                            </thead>
                            <tbody>
                                <row>
                                    <entry>
                                        <p><?oxy-placeholder content=" Condition#1"?></p>
                                    </entry>
                                    <entry>
                                        <p><?oxy-placeholder content=" Condition#2"?></p>
                                    </entry>
                                    <entry>
                                        <p><?oxy-placeholder content=" Action#1"?></p>
                                    </entry>
                                    <entry>
                                        <p><?oxy-placeholder content=" Action#2"?></p>
                                    </entry>
                                </row>
                                <row>
                                    <entry>
                                        <p/>
                                    </entry>
                                    <entry>
                                        <p/>
                                    </entry>
                                    <entry>
                                        <p/>
                                    </entry>
                                    <entry>
                                        <p/>
                                    </entry>
                                </row>
                            </tbody>
                        </tgroup>
                    </table>
                    <p><?oxy-placeholder content=" Additional information to understand this process stage."?></p>
                    <p><?oxy-placeholder content=" Specify the result of this stage."?></p>
                </process_stage>
            </process_stages>
        </process_workflow>
        <result>
            <p><?oxy-placeholder content=" Add it only if it provides valuable direction to the reader, and is not obvious from the topic title and not captured in another block."?></p>
        </result>
        <whatsnext>
            <p><?oxy-placeholder content=" Section pointing to the subsequent related process, if any."?></p>
        </whatsnext>
    </ct_process_body>
</ct_process>
